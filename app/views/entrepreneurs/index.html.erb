<h1>Listing entrepreneurs</h1>

<table>
  <tr>
    <th>First name</th>
    <th>Last name</th>
    <th>Gender</th>
    <th>Document type</th>
    <th>Document number</th>
    <th>Email</th>
    <th>Cellphone</th>
    <th>Address</th>
    <th>Born at</th>
    <th>Password</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @entrepreneurs.each do |entrepreneur| %>
  <tr>
    <td><%= entrepreneur.first_name %></td>
    <td><%= entrepreneur.last_name %></td>
    <td><%= entrepreneur.gender %></td>
    <td><%= entrepreneur.document_type %></td>
    <td><%= entrepreneur.document_number %></td>
    <td><%= entrepreneur.email %></td>
    <td><%= entrepreneur.cellphone %></td>
    <td><%= entrepreneur.address %></td>
    <td><%= entrepreneur.born_at %></td>
    <td><%= entrepreneur.password %></td>
    <td><%= link_to 'Show', entrepreneur %></td>
    <td><%= link_to 'Edit', edit_entrepreneur_path(entrepreneur) %></td>
    <td><%= link_to 'Destroy', entrepreneur, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Entrepreneur', new_entrepreneur_path %>
